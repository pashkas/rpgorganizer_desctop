                                            </TransformGroup>
                                        </Path.RenderTransform>
                                    </Path>
                                </Grid>
                            </Viewbox>
                        </ContentControl>
                    </MenuItem.Icon>
                </MenuItem>

                <MenuItem
                    Width="{StaticResource MenuIconSize}"
                    Height="{StaticResource MenuIconSize}"
                    ToolTip="Выход">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="Click">
                            <ei:CallMethodAction MethodName="Exit" TargetObject="{Binding}" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                    <MenuItem.Icon>
                        <ContentControl>
                            <Viewbox Margin="-2" Stretch="Uniform">

                                <Grid>
                                    <Grid
                                        Width="128"
                                        Height="128"
                                        Visibility="Collapsed" />
