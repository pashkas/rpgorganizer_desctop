                        Content=" сегодня /"
                        DockPanel.Dock="Right"
                        Foreground="White"
                        Style="{StaticResource Button-Sketch}" />-->

                <Grid>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" MinWidth="200" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <StackPanel
                        Grid.Column="1"
                        Margin="15,0"
                        Orientation="Horizontal"
                        Visibility="{Binding Pers.IsSleepForNewLevel, Converter={StaticResource BoolToVisibilityConverter}}">

                        <svgc:SvgViewbox
                            Height="25"
                            Source="/Images/MarioMashroom.svg"
                            Stretch="Uniform"
                            ToolTip="После сна можно будет прокачать навыки!" />
                        <TextBlock
                            VerticalAlignment="Center"
                            FontSize="16"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="  zzz!"
                            TextAlignment="Center"
                            ToolTip="После сна можно будет прокачать навыки!" />
                    </StackPanel>

                    <UniformGrid Grid.Column="0" Rows="1">
                        <!--  EXTENDEDVERS  -->
                        <Button
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Command="{Binding DataContext.AddNewTask, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                            CommandParameter="{Binding}"
                            Style="{StaticResource Button-Sketch}"
                            ToolTip="Создать монстра"
                            Visibility="{Binding DataContext.Pers.IsSettingsVisibillity, RelativeSource={RelativeSource AncestorType=UserControl}}">
                            <ContentControl
                                Height="30"
                                Margin="4"
                                Template="{StaticResource ContentControlAddNew}" />
                        </Button>
                        <Button
                            Grid.Column="0"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Command="{Binding EndOfTurnCommand}"
                            Foreground="White"
                            Style="{StaticResource Button-Sketch}"
                            ToolTip="Завершить ход">
                            <DockPanel>
                                <ContentControl
                                    Height="25"
                                    Margin="4"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource SandWatchStyle}" />
                            </DockPanel>
                        </Button>
                    </UniformGrid>

                    <DatePicker
                        Grid.Column="2"
                        VerticalAlignment="Center"
                        BorderBrush="{StaticResource BaseBorder-Sketch}"
                        BorderThickness="{StaticResource BaseBorderThickness}"
                        DockPanel.Dock="Right"
                        FontFamily="{StaticResource FontFamily-Sketch}"
                        FontSize="{StaticResource SizeDouble-Sketch}"
                        Foreground="{StaticResource BaseForeground-Sketch}"
                        SelectedDate="{Binding selectedDateTime}"
                        SelectedDateFormat="Long"
                        ToolTip="Игровая дата. Не забудь нажать 'завершить ход' в конце дня!" />

                    <UniformGrid Grid.Column="3" Rows="1">
                        <Button
                            Margin="1,0"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Command="{Binding FocModeEnabledDisabledCommand}"
                            Style="{StaticResource Button-Sketch}"
                            ToolTip="Активировать/деактивировать режим фокусировки"
                            Visibility="Collapsed">
                            <!--  Visibility="{Binding IsPlaningMode, Converter={StaticResource BoolToVisibilityConverter}, ConverterParameter=инверсия}"  -->
                            <svgc:SvgViewbox
                                Height="22"
                                Source="Images/FocusAim.svg"
                                Stretch="Uniform" />
                        </Button>

                        <Button
                            Margin="0,1"
                            Background="Transparent"
                            BorderThickness="2"
                            Command="{Binding SetPlaningModeCommand}"
                            ToolTip="Режим планирования (будут отображены все задачи не смотря на последовательность и дату)">
                            <Button.Style>
                                <Style BasedOn="{StaticResource Button-Sketch}" TargetType="Button">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsPlaningMode}" Value="True">
                                            <Setter Property="BorderBrush" Value="LightGreen" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding IsPlaningMode}" Value="False">
                                            <Setter Property="BorderBrush" Value="Transparent" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <ContentControl Height="22" Margin="4">
                                <Viewbox>
                                    <Grid>
                                        <Grid
                                            Width="128"
                                            Height="128"
                                            Visibility="Collapsed" />
                                        <Path
                                            Width="68"
                                            Height="68"
