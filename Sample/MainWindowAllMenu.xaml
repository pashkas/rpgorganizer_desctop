<UserControl
    x:Class="Sample.MainWindowAllMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:View="clr-namespace:Sample.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:model="clr-namespace:Sample.Model"
    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
    x:Name="UserControl"
    d:DesignHeight="64"
    d:DesignWidth="1200"
    UseLayoutRounding="True"
    mc:Ignorable="d">
    <UserControl.DataContext>
        <Binding Path="MainVM" Source="{StaticResource Locator}" />
    </UserControl.DataContext>
    <UserControl.Resources>
        <Style TargetType="{x:Type DatePickerTextBox}">
            <Setter Property="TextAlignment" Value="Center" />
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <TextBox
                            x:Name="PART_TextBox"
                            Foreground="{StaticResource BaseForeground-Sketch}"
                            Text="{Binding Path=SelectedDate, ConverterCulture='ru-RU', StringFormat='dddd (dd/MM/yy)', RelativeSource={RelativeSource AncestorType={x:Type DatePicker}}}" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <model:IsElementVisibleConverter x:Key="IsElementVisibleConverter" />
    </UserControl.Resources>

    <Grid Background="{StaticResource TopBrush}">
        <Grid Margin="4,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid>
                <Menu
                    VerticalAlignment="Center"
                    Background="Transparent"
                    ItemContainerStyle="{StaticResource TopMenu}">
                    <MenuItem
                        MinWidth="{StaticResource MenuIconSize}"
                        Command="{Binding OpenQwickButtonCommand}"
                        CommandParameter="Информация"
                        FontSize="22"
                        FontWeight="Bold"
                        Foreground="White"
                        Header="П"
                        ToolTip="Открыть информацию о персонаже"
                        Visibility="Collapsed" />
                    <!--  EndRegion  -->
