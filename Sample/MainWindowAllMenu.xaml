                        BorderBrush="Transparent"
                        Command="{Binding QwickSetDateCommand}"
                        CommandParameter="tommorow"
                        Content=" завтра"
                        DockPanel.Dock="Right"
                        Foreground="White"
                        Style="{StaticResource Button-Sketch}" />
                <Button Background="Transparent"
