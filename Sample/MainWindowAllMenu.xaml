                    Height="{StaticResource MenuIconSize}"
                    Click="MenuItem_Click_1"
                    ToolTip="Скрыть">
                    <MenuItem.Icon>
                        <ContentControl>
                            <Viewbox Margin="-2" Stretch="Uniform">
                                <Grid>
                                    <Grid
                                        Width="128"
                                        Height="128"
